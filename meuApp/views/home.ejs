    <h1 class="title-contact bg-light" id="contato">Contato</h1>
    <div class="site-section bg-light" id="contato" data-aos="fade-up">
      <div class="container">
        <div class="row align-items-first">
          <div class="col-md-7">
            <form action="/saveContact" method="POST" class="bg-white">

              <div class="p-3 p-lg-5 border">
                <div class="form-group row">
                  <div class="col-md-6">
                    <label for="c_fname" class="text-black">Nome <span class="text-danger">*</span></label>
                    <input type="text" class="form-control mixed-mask" id="c_fname" name="c_fname"
                      placeholder="Ex.: João ">
                  </div>
                  <div class="col-md-6">
                    <label for="c_lname" class="text-black">Sobrenome <span class="text-danger">*</span></label>
                    <input type="text" class="form-control mixed-mask" id="c_lname" name="c_lname"
                      placeholder="Ex.: Ribeiro da Silva">
                  </div>
                </div>
                <div class="form-group row">
                  <div class="col-md-12">
                    <label for="c_email" class="text-black">Email <span class="text-danger">*</span></label>
                    <input type="email" class="form-control mixed-mask" id="c_email" name="c_email"
                      placeholder="Ex.: seu_email@gmail.com">
                  </div>
                </div>
                <div class="form-group row">
                  <div class="col-md-12">
                    <label for="c_phone" class="number">Telefone </label>
                    <input type="text" class="form-control phone-ddd-mask" placeholder="Ex.: (00) 0000-0000" name="c_phone">
                  </div>
                </div>

                <div class="form-group row">
                  <div class="col-md-12">
                    <label for="c_message" class="text-black">Mensagem </label>
                    <textarea name="c_message" id="c_message" cols="30" rows="7" class="form-control"></textarea>
                  </div>
                </div>
                <div class="form-group row">
                  <div class="col-lg-12">
                    <input type="submit" class="btn btn-danger btn-lg btn-block" value="Enviar Mensagem">
                  </div>
                </div>
              </div>
            </form>
          </div>

          <div class="col-md-5">
            <div class="p-4 border mb-3 bg-white">
              <p class="mb-0">Endereço</p>
